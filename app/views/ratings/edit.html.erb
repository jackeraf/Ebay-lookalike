
<div class="col-md-10 col-md-offset-1">
	<div class="edit-review-form">
	<h1> Edit rating </h1>
		<%= simple_form_for([@product, @rating]) do |f| %>
	
		<div id= "rating-form">
			<label>Rating</label>
		</div>

		<%= f.button :submit, class: "btn-custom2"  %>
		<% end %>
	</div>
</div>

<script type="text/javascript">
// this is the not read only script
	$("#rating-form").raty({
		path: '/assets',
		scoreName: 'rating[rating]'
	});


</script>

